{
  "name": "its-brunch-time",
  "description": "A simple brunch project for rapid Front End development",
  "private": true,
  "author": "Apostolis Kyromites",
  "authorURI": "http://apokyro.com/",
  "license": "MIT",
  "version": "0.1.0",
  "repository": "",
  "scripts": {
    "start": "brunch watch --server",
    "build": "brunch build --production"
  },
  "engines": {
    "node": ">=6.11.0"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2",
    "babel-brunch": "^6",
    "babel-eslint": "^8",
    "babel-preset-env": "^1",
    "brunch": "^2",
    "brunch-static": "^1.2.1",
    "eslint": "^4",
    "eslint-config-airbnb-base": "^12",
    "eslint-config-prettier": "^2",
    "eslint-plugin-import": "^2",
    "eslint-plugin-prettier": "^2",
    "html-brunch-static": "^1.4.0",
    "pleeease-brunch": "^3",
    "prettier": "^1",
    "sass-brunch": "^2",
    "uglify-js-brunch": "^2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "import",
      "prettier"
    ],
    "env": {
      "jquery": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
